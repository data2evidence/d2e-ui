# Accessible URL to the public folder. Ref: https://create-react-app.dev/docs/using-the-public-folder/
PUBLIC_URL=/superadmin
# Central service base URL
REACT_APP_CS_BASE_URL=https://localhost:4100/
# OAuth client ID of the shared Azure AD
REACT_APP_OAUTH_SHARED_AD_CLIENT_ID=ae3538d6-1767-42ad-84f6-777e6d13a83b
# OAuth authority of the shared Azure AD
REACT_APP_OAUTH_SHARED_AD_AUTHORITY=https://login.microsoftonline.com/e0b44b06-f118-486b-9be8-0c02b05b0adf/
# OAuth redirect URI of the shared Azure AD after logged in
REACT_APP_OAUTH_SHARED_AD_REDIRECT_URI=https://localhost:4100/superadmin
# OAuth redirect URI of the shared Azure AD after logout
REACT_APP_OAUTH_SHARED_AD_POST_LOGOUT_REDIRECT_URI=https://localhost:4100/superadmin
# Date time format
REACT_APP_DATETIME_FORMAT=DD.MM.YYYY (ddd) HH:mm:ss
# Azure group ID for ALP owner role
REACT_APP_ALP_OWNER_GROUP_ID=f08de65f-7d2c-4c5e-b30c-5378ede16a36
# Azure group ID for super admin role
REACT_APP_SUPERADMIN_GROUP_ID=ea49a27d-8586-4543-82dd-0b74f358d60f

# Port number for local development
PORT=4100
# Flag to enable https for local development
HTTPS=true
# Local SSL Setup
SSL_KEY_FILE="../../.cert/local_alp_portal_private.key"
SSL_CRT_FILE="../../.cert/local_alp_portal_public.crt"

# Required only when running yarn start in superadmin folder for debugging
