{
  "name": "portal",
  "version": "1.0.0",
  "private": true,
  "homepage": "/portal",
  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=/portal BUILD_PATH='../../resources/portal' react-scripts build",
    "build-mri": "npx grunt production",
    "build-mri-css": "npx grunt css",
    "lint": "tsc --noEmit && prettier --check . && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "watch": "nodemon --watch \"src/**\" --ext \"tsx,ts\" --exec \"yarn build\"",
    "prettier": "prettier --write \"**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@alp-os/alp-starboard-notebook": "git+ssh://git@github.com/alp-os/alp-starboard-notebook-base.git#d4l-0.15.7-develop",
    "@alp-os/alp-starboard-wrap": "git+ssh://git@github.com/alp-os/alp-starboard-wrap.git#develop",
    "@axa-fr/react-oidc": "^6.10.9",
    "@d4l/web-components-library": "^3.20.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.3",
    "@mui/material": "^5.8.3",
    "@mui/x-date-pickers": "^6.19.2",
    "@portal/components": "^1.0.0",
    "@portal/plugin": "^1.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/papaparse": "^5.3.2",
    "@types/react-csv": "^1.1.2",
    "axios": "^0.27.2",
    "bootstrap": "^5.0.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.3",
    "easymde": "^2.16.1",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "file-saver": "^2.0.5",
    "gethue": "^5.0.0",
    "jszip": "3.10.1",
    "lodash": "^4.17.21",
    "material-react-table": "^2.9.2",
    "pako": "^1.0.11",
    "papaparse": "^5.3.2",
    "randombytes": "^2.1.0",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.3.0",
    "react-simplemde-editor": "^5.0.2",
    "systemjs": "0.21.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/bootstrap": "^5.1.13",
    "@types/file-saver": "^2.0.5",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.7.13",
    "@types/pako": "^1.0.1",
    "@types/papaparse": "^5.3.2",
    "@types/randombytes": "^2.0.3",
    "@types/react": "^18.0.0",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "grunt": "^1.5.3",
    "grunt-cli": "^1.3.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^2.1.0",
    "grunt-openui5": "^0.18.4",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "^2.6.2",
    "react-scripts": "5.0.1",
    "react-test-renderer": "18.2.0",
    "sass": "^1.52.3",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(react-markdown|vfile|vfile-message|markdown-table|unist-.*|unified|bail|is-plain-obj|trough|remark-.*|mdast-util-.*|escape-string-regexp|micromark.*|decode-named-character-reference|character-entities|property-information|hast-util-whitespace|space-separated-tokens|comma-separated-tokens|pretty-bytes|ccount|rehype-*|hast-util-.*|mdast-util-gfm|web-namespaces|zwitch|hast-to-hyperscript|html-void-elements|gemoji)).+\\.(js|jsx|mjs|cjs|ts|tsx)$"
    ]
  }
}
