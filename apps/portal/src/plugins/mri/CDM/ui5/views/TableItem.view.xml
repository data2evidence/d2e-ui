<mvc:View controllerName="hc.hph.cdw.config.ui.views.TableItem"
	width="100%" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.commons.layout" xmlns:html="http://www.w3.org/1999/xhtml">

<html:div width="100%" class="sapMxConfigListItem">
<Select class="sapTriggerSave sapMxConfigLangKey" enabled="{path:'editableModel>/editable',mode:'oneWay'}" selectedKey="{configEditorModel>placeholder}" id="tableDD" width="28%"/>
<Input class="sapTriggerSave sapMxConfigLangValue" enabled="{path:'editableModel>/editable',mode:'oneWay'}" value="{configEditorModel>table}" tooltip = "{configEditorModel>table}" width="60%" valueLiveUpdate="true"/>
<Button class="sapTriggerSave sapMxConfigLangButton" icon="sap-icon://delete" tooltip="{hc.hph.cdw.config.ui.i18n>HPH_CDM_CFG_REMOVE}" 
            enabled="{path:'editableModel>/editable',mode:'oneWay'}" press="onRemovePressed" type="Transparent"/>
</html:div>


</mvc:View>
